{
  "name": "@bluewhale/vue-ui-lib",
  "version": "1.0.0",
  "description": "BlueWhale Vue 3 Component Library - A modern and elegant UI library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "type-check": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "components",
    "ui",
    "library",
    "bluewhale",
    "typescript",
    "frontend"
  ],
  "author": "BlueWhale Team",
  "license": "MIT"
}